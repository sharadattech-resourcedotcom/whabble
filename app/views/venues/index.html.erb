<p id="notice"><%= notice %></p>

<h1>Venues</h1>

<table>
  <thead>
    <tr>
      <th>Venuename</th>
      <th>Venuedescription</th>
      <th>Venuetagline</th>
      <th>Venueurl</th>
      <th>Venuephone</th>
      <th>Venueemail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.venuename %></td>
        <td><%= venue.venuedescription %></td>
        <td><%= venue.venuetagline %></td>
        <td><%= venue.venueurl %></td>
        <td><%= venue.venuephone %></td>
        <td><%= venue.venueemail %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>
