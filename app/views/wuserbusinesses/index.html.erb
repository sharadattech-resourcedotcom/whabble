<h1>Wuserbusinesses</h1>

<table>
  <thead>
    <tr>
      <th>Wuserco</th>
      <th>Wusercoadd1</th>
      <th>Wusercoadd2</th>
      <th>Wusercocity</th>
      <th>Wusercostate</th>
      <th>Wusercopostal</th>
      <th>Wusercophone</th>
      <th>Wusercoemail</th>
      <th>Wusercotitle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wuserbusinesses.each do |wuserbusiness| %>
      <tr>
        <td><%= wuserbusiness.Wuserco %></td>
