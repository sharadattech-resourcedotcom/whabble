        <td><%= wuserbusiness.wusercotitle %></td>
        <td><%= link_to 'Show', wuserbusiness %></td>
        <td><%= link_to 'Edit', edit_wuserbusiness_path(wuserbusiness) %></td>
        <td><%= link_to 'Destroy', wuserbusiness, method: :delete, data: { confirm: 'Are you sure?' } %></td>
